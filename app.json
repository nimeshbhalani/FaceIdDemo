{
  "expo": {
    "name": "faceIdDemo",
    "slug": "faceIdDemo",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.iblinfotech.app.TestAppInExpo",
      "buildNumber": "1",
      "infoPlist": {
        "NSFaceIDUsageDescription": "IBL wants to use your face id"
      }
    }
  }
}
